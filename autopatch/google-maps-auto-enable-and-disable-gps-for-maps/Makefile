NAME = $(shell basename $(shell pwd))
PATCH = google-maps/${NAME}.patch
TITLE = Auto Enable and Disable GPS for Maps
DESCRIPTION = This will turn on GPS when launching Maps and then turn it off, when exiting Maps. If GPS already on, when launching Maps, it will not turn GPS off, when exiting Maps.
CATEGORY = Google Maps
VERSIONS = 1.4.5-8 2.0.0-45 2.0.1-73 2.1.0-63 2.1.1-22 2.1.2-31
